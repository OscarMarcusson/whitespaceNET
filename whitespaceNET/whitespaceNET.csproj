<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Oscar_Marcusson</Authors>
    <Version>1.0.0</Version>
    <Description>A C# utility that enables parsing of significant whitespace text, like python or yaml files</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/OscarMarcusson/whitespaceNET</RepositoryUrl>
    <PackageTags>whitespace,parser,hierarchy,tabs,spaces,indentation</PackageTags>
    <PackageIcon>whitespaceNET-package-icon.png</PackageIcon>
  </PropertyGroup>
	
  <ItemGroup>
    <None Include="..\Icons\whitespaceNET-package-icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
	
</Project>
